module blog.post;

/// Structure representing a blog post.
struct BlogPost
{
    string date         = "";   /// Date that the blog post was posted
    string name         = "";   /// Name of the blog post
    string description  = "";   /// Description of the blog post
    string content      = "";   /// Actual content of the blog post
    string url          = "";   /// Blog post url
    string author       = "";   /// Author of the blog post
}
